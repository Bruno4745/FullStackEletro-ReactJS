rios\";\r\n        await fetch(url, {\r\n            method: \"POST\",\r\n            headers: {\r\n              \"Content-Type\": \"application/json\"\r\n            },\r\n            body: JSON.stringify(dadosForm)\r\n        }); \r\n    }\r\n    //FIM - envia dados do formulario para API inserir no BD\r\n    \r\n    return(\r\n        <Container>\r\n            <Jumbotron className=\"bg-white pb-2\">\r\n                <h1>Contato</h1>\r\n                <hr className=\"my-4\"/>\r\n                <div className=\"row text-center my-5\">\r\n                    <div className=\"col col-12 col-sm-6 mt-2\">\r\n                        <img src={ImgWhats} alt=\"Email\" width=\"70\"/>\r\n                        <p>contato@fullstackeletro.com</p>\r\n                    </div>\r\n                    <div className=\"col col-12 col-sm-6\">\r\n                        <img src={ImgEmail} alt=\"WhatsApp\" width=\"70\"/>\r\n                        <p>(11) 2222-3333</p>\r\n                    </div>\r\n                </div>\r\n            </Jumbotron>\r\n            <Form onSubmit={enviaMensagem} className=\"col-12 col-md-10 mx-auto mb-5\">\r\n                <h3>Entre em contato conosco!!!</h3>\r\n                <Form.Group>\r\n                    <Form.Label>Nome</Form.Label>\r\n                    <Form.Control id=\"nome\" name=\"nome\" className=\"mb-3\" type=\"text\" placeholder=\"Digite seu nome\" onChange={handleChange} required />\r\n                </Form.Group>\r\n                <Form.Group>\r\n                    <Form.Label>Mensagem</Form.Label>\r\n                    <Form.Control id=\"mensagem\" name=\"mensagem\" className=\"mb-3\" as=\"textarea\" rows={3} placeholder=\"Digite uma mensagem\" onChange={handleChange} required />\r\n                </Form.Group>\r\n                <Form.Group>\r\n                    <Button type=\"submit\" variant=\"success\">Enviar</Button>\r\n                    {'  '}\r\n                    <Button type=\"reset\" variant=\"warning\">Limpar</Button>\r\n                </Form.Group>\r\n            </Form>\r\n            <Table striped bordered hover>\r\n                <thead>\r\n                    <tr>\r\n                        <th>ID</th>\r\n                        <th>Nome</th>\r\n                        <th>Comentario</th>\r\n                        <th>Data</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {comentarios && comentarios.map(item => <Comentarios key={item.idcomentario} id={item.idcomentario} nome={item.nome} comentario={item.mensagem} data={item.data} />)}\r\n                </tbody>\r\n            </Table>\r\n        </Container>\r\n    )\r\n}","C:\\reactprojeto\\fullstackeletro\\src\\Redux\\reducers.js",[],"C:\\reactprojeto\\fullstackeletro\\src\\Redux\\todosProdutos.js",["86"],"C:\\reactprojeto\\fullstackeletro\\src\\Redux\\store.js",[],"C:\\reactprojeto\\fullstackeletro\\src\\Redux\\carrinhoReducer.js",["87"],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":22,"column":23,"nodeType":"94","messageId":"95","endLine":22,"endColumn":35},{"ruleId":"88","replacedBy":"96"},{"ruleId":"90","replacedBy":"97"},{"ruleId":"88","replacedBy":"98"},{"ruleId":"90","replacedBy":"99"},{"ruleId":"88","replacedBy":"100"},{"ruleId":"90","replacedBy":"101"},{"ruleId":"92","severity":1,"message":"93","line":24,"column":23,"nodeType":"94","messageId":"95","endLine":24,"endColumn":35},{"ruleId":"102","severity":1,"message":"103","line":1,"column":1,"nodeType":"104","endLine":74,"endColumn":4},{"ruleId":"102","severity":1,"message":"105","line":5,"column":1,"nodeType":"104","endLine":37,"endColumn":2},"no-native-reassign",["106"],"no-negated-in-lhs",["107"],"no-unused-vars","'setDadosForm' is assigned a value but never used.","Identifier","unusedVar",["106"],["107"],["106"],["107"],["106"],["107"],"import/no-anonymous-default-export","Assign array to a variable b